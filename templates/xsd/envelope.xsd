<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.w3.org/2003/05/soap-envelope" xmlns:w="http://www.oorsprong.org/websamples.countryinfo" xmlns:soap12="http://www.w3.org/2003/05/soap-envelope">
    <xs:import namespace="http://www.oorsprong.org/websamples.countryinfo" schemaLocation="{{ operation_xsd }}"/>
    <xs:element name="Envelope">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="soap12:Body"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Body">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="w:{{ operation }}"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>